{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@/components/*": ["./src/components/*"],
        "@/lib/*": ["./src/lib/*"],
        "@/styles/*": ["./src/styles/*"],
        "@/utils/*": ["./src/utils/*"]
      },
      "jsx": "react-jsx"
    },
    "include": ["src/**/*.js", "src/**/*.jsx"],
    "exclude": ["node_modules"]
  }